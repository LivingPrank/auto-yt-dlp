{
	"manifest_version": 2,
	"name": "auto-yt-dlp",
	"description": "Downloads all open youtube and youtube music tabs' audio",
	"version": "1.0",
	"background": {
		"scripts" : ["background.js"]
	},
	"browser_action" : {
		"default_icon" : "icon.png"
	},
	"permissions" : ["activeTab", "tabs"]
}